# PostgreSQL database connection string
DATABASE_URL=postgres://user:pass@localhost:5432/aitoolcatalog

# Used for Replit authentication or localhost development fallback
REPLIT_DOMAINS=localhost:3000

# Session secret used for encrypting session cookies or tokens
SESSION_SECRET=your-secret-key-here

# Port for Express server (optional if hardcoded in your code as 5000)
PORT=5000

# OpenAI API key for AI-enhanced search (replace with your key)
OPENAI_API_KEY=your-openai-api-key-here
